{
  "name": "redux-worker-queue",
  "version": "1.2.0",
  "description": "Redux powered queue for the processing of serializable objects through sequential custom handlers, by parallel workers, with error and endless loop protection. Aimed at complex deferred offline persistence handling.",
  "main": "./lib/index.js",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "build": "tsc"
  },
  "keywords": [
    "redux",
    "queue",
    "offline",
    "offline-first",
    "typescript",
    "TDD",
    "worker"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/heysailor/redux-worker-queue.git"
  },
  "author": "Nick McIntosh",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/lodash": "^4.14.105",
    "@types/md5": "^2.1.32",
    "@types/shortid": "^0.0.29",
    "coveralls": "^3.0.0",
    "jest": "^22.4.2",
    "jest-mock-console": "^0.3.5",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "md5": "^2.2.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "shortid": "^2.2.8"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "lib/*.*"
    ],
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "jest-mock-console/dist/setupTestFramework.js"
  }
}
